{
  "name": "teutonic-css",
  "version": "0.7.14",
  "description": "Yet another CSS framework — versatile, well documented.",
  "directories": {},
  "scripts": {
    "prepublishOnly": "gulp",
    "node-sass": "node-sass teutonic.scss teutonic-new.css",
    "create-css": "sass teutonic.scss teutonic-new.css --no-source-map",
    "minify-css": "cleancss --output teutonic-new.min.css teutonic-new.css",
    "gzip": "gzip teutonic-new.min.css",
    "build": "npm run create-css && npm run minify-css && npm run gzip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fortrabbit/teutonic-css.git"
  },
  "keywords": [
    "CSS grid",
    "CSS vars",
    "Flexbox",
    "CSS Custom Properties",
    "SCSS",
    "framework"
  ],
  "unpkg": "teutonic.min.css",
  "author": "Frank Lämmer, <frank@franklaemmer.de>, (https://www.franklaemmer.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fortrabbit/teutonic-css/issues"
  },
  "homepage": "https://teutonic.co",
  "devDependencies": {
    "clean-css-cli": "^5.3.0",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-gzip": "^1.4.2",
    "gulp-header": "^2.0.9",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "hoek": "~6.1",
    "node-sass": "^6.0.0",
    "sass": "^1.32.12"
  }
}
